{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/AttachR/bin/Debug/AttachR.exe",
			"args": [],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": true
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processName": "AttachR.exe"
		}
	]
}